import RPi.GPIO as GPIO
import time
GPIO.setmode(GPIO.BCM)
GPIO.setup(27, GPIO.OUT)
try:
    while 1:
        GPIO.output(27, 1)
        print ("led is on")
        time.sleep(0.25)
        GPIO.output(27, 0)
        print ("led is off")
        time.sleep(0.25)
except KeyboardInterrupt:
    GPIO.cleanup()
